; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="XP Skeleton"
config/description="A skeleton project made to bootstrap Godot 4 games, specifically focusing around 2D pixel art games. Implements a main menu, pause menu, settings, and saves/loads settings to a ConfigFile resource on disk. Also includes a ton of helper \"standard library\" additions and gameplay systems under xplib.
Godot version: 4.4-stable.
"
config/tags=PackedStringArray("library")
run/main_scene="res://src/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.4", "Mobile")
config/icon="res://assets/icon.svg"

[autoload]

Enum="*res://xpdata/Enum.gd"
Constants="*res://xpdata/Constants.gd"
Global="*res://global.gd"
PLog="*res://xplib/PLog.gd"
Sound="*res://sound.gd"
PNumber="*res://xplib/PNumber.gd"
PString="*res://xplib/PString.gd"
PList="*res://xplib/PList.gd"
PMath="*res://xplib/PMath.gd"
PMisc="*res://xplib/PMisc.gd"
PPlatform="*res://xplib/PPlatform.gd"
PTiming="*res://xplib/PTiming.gd"
SignalBus="*res://xplib/systems/SignalBus.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/handheld/orientation=4

[editor]

script/templates_search_path="res://xplib"

[editor_plugins]

enabled=PackedStringArray("res://addons/gut/plugin.cfg", "res://addons/script-ide/plugin.cfg", "res://addons/table_for_scene_tabs/plugin.cfg")

